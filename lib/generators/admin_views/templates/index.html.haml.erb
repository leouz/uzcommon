.page-header
  %h1 <%= @model_name.pluralize %>
= link_to "New <%= @model_name %>", new_admin_<%= @model_name.underscore %>_path, :class => 'btn btn-primary'
%table.table.table-striped
  %thead
    %tr
<% @columns.each do |p| %>      %th <%= p[:name].humanize %>
<% end %>
      %th
  %tbody
    - @<%= @model_name.underscore.pluralize %>.each do |<%= @model_name.underscore %>|
      %tr
<% @columns.each do |p| %>        %td= <%= "#{@model_name.underscore}.#{p[:name]}" %>
<% end %>
        %td= bs_display(:links, { :edit => edit_admin_<%= @model_name.underscore %>_path(<%= @model_name.underscore %>), :delete => admin_<%= @model_name.underscore %>_path(<%= @model_name.underscore %>) })